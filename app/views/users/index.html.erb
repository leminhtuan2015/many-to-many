<%= link_to "New user", new_user_path%>

<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.id %></th>
          <td><%= link_to user.name, user_path(user) %>
          </td>
          <td><%= link_to "Add skills", user_user_skills_path(user) %>
          </td>
      </tr>
    <% end %> 
  </tbody>
</table>